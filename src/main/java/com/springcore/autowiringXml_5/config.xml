<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="studCity" class="com.springcore.autowiringXml_5.City">
		<property name="cityName" value="Sangli"/>
	</bean>
	
	<bean id="studCity1" class="com.springcore.autowiringXml_5.City">
		<property name="cityName" value="Sangli"/>
	</bean>
	
	<bean id="student" class="com.springcore.autowiringXml_5.Student" autowire="byName"><!-- byName, byType, constructor -->
		<property name="studId" value="102"/>
		<property name="studName" value="Akshay Malkar"/>
	</bean>

</beans>
